    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "3.5.2",
    "ts-json-schema-generator": "^2.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bluebird": "^3.7.2",
